    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "rating": {
      "type": "decimal"
    },
    "thumbnail": {
      "type": "string"
    },
    "businessId": {
      "type": "string"
    },
    "productId": {
      "type": "string"
    },
    "modelsNumber": {
      "type": "integer"
    },
    "tryoutLink": {
      "type": "string"
    },
    "testImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "arViewer": {
      "type": "string"
    },
