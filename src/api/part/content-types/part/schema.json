{
  "kind": "collectionType",
  "collectionName": "parts",
  "info": {
    "singularName": "part",
    "pluralName": "parts",
    "displayName": "Part",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "cover": {
      "type": "boolean"
    },
    "displayname": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "chartType": {
      "type": "enumeration",
      "enum": [
        "none",
        "CircularProgress001",
        "CircularProgress003",
        "CircularProgress004",
        "Wavecircle1",
        "BarChart",
        "BarChartTwoColumn01",
        "BarChartTwoColumn02",
        "Dashboard003"
      ]
    },
    "unit": {
      "type": "string"
    },
    "minValue": {
      "type": "float"
    },
    "maxValue": {
      "type": "float"
    },
    "sensorName": {
      "type": "string"
    },
    "chartSize": {
      "type": "enumeration",
      "enum": [
        "Large",
        "Small"
      ]
    },
    "chartOrder": {
      "type": "integer"
    },
    "asset": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::asset.asset",
      "inversedBy": "parts"
    },
    "part_interactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::part-interaction.part-interaction",
      "mappedBy": "part"
    }
  }
}
